{
  "name": "elevatorsaga",
  "version": "1.0.0",
  "description": "Elevator Saga =================== The elevator programming game",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "vite build",
    "postbuild": "cp -r font-awesome-4.1-1.0/fonts dist/ && cp -r libs dist/ && cp *.css dist/ && cp *.md dist/ && cp -r images dist/",
    "serve": "vite preview --port 3000 --strictPort",
    "dev": "vite",
    "pretest": "npm run build",
    "test:jasmine": "node --experimental-vm-modules node_modules/jasmine/bin/jasmine.js",
    "test": "npm run test:jasmine && playwright test",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf test-results && rm -rf playwright_report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tdurtschi/elevator-saga-2.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/tdurtschi/elevator-saga-2/issues"
  },
  "homepage": "https://github.com/tdurtschi/elevator-saga-2#readme",
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/node": "^22.15.18",
    "esbuild": "^0.14.38",
    "jasmine": "^5.1.0",
    "jsdom": "^22.1.0",
    "vite": "^5.4.8"
  },
  "dependencies": {
    "@mlc-ai/web-llm": "^0.2.79",
    "jquery": "^2.1.1",
    "lodash": "^3.6.0",
    "lodash-es": "^4.17.21",
    "openai": "^6.7.0",
    "serve": "^13.0.2",
    "sweetalert2": "^11.26.3"
  }
}
